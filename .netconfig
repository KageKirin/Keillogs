logFilePath = "nugettier.log"

[logging "LogLevel"]
Default = "Error"
Microsoft = "Warning"

[guid]
algorithm = "xxhash128"
format = "unity"

[defaults]
unity = "2022.3"
target = "https://foo.bar"
output-directory = "."

[package ""]
name = "external.{{{Authors}}}.{{{Identity.Id}}}"

[package "keillogs.*"]
name = "com.kagekirin.{{{Identity.Id}}}"
recurse = true


[unity "2023.*"]
framework = "netstandard2.1"

[unity "2022.*"]
framework = "netstandard2.1"

[unity "2021.*"]
framework = "netstandard2.0"

[source "api.nuget.org/v3/index.json"]
protocol = "https"
